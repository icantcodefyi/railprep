{
  "buildCommand": "bun run build",
  "outputDirectory": "dist/client",
  "devCommand": "bun run dev",
  "installCommand": "bun install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
